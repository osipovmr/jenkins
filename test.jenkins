#!groovy

properties([disableConcurrentBuilds()])
pipeline {
    agent any
    options {
        timestamps()
    }
    tools {
        maven 'Maven 3.8.4'
    }
    stages {
        stage('Checkout') {
            steps {
                checkout scm
            }
        }
        stage('Build JAR') {
            steps {
                withMaven {
                    sh "mvn clean package"
                }
            }
        }
    }
}